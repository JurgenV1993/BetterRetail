<div data-templateid="ShippingAddressRegistered">
    {{#if_eq IsAuthenticated true}}
        <section class="card" aria-label="{{localize 'CheckoutProcess' 'T_ShippingAddress'}}">
            <div class="card-header text-white bg-dark">
                <h2 class="h3">{{localize 'CheckoutProcess' 'T_ShippingAddress'}}</h2>
            </div>
            <div class="card-body">
                {{#if_eq GettingCart true}}
                    {{>Loading}}
                {{/if_eq}}
                {{#if_eq GettingCart false}}
                    {{> RegisteredAddresses}}
                {{/if_eq}}
            </div>
        </section>
    {{/if_eq}}
</div>