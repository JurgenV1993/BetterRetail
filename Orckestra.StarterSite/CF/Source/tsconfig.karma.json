  "compilerOptions": {
    "declaration": true,
    "allowUnusedLabels": false,
    "noResolve": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noLib": false,
    "removeComments": false,
    "target": "ES5",
    "outDir": "./.temp/Tests/",
    "reorderFiles": true
  },
  "include": [
