<?xml version="1.0"?>
<!--  
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
Full documentation for CTT transforms http://msdn.microsoft.com/en-us/library/dd465326.aspx 
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
-->
<transforms xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<SetAttributeValueTransform
	    url="https://{ocs-cm-hostName}/api"
	    authToken="{ocsAuthToken}"
	    xdt:Transform="SetAttributes(url, authToken)"
	    xdt:Locator="XPath(/settings/overture)" />

	<SetAttributeValueTransform
		jobCronSchedule="0 2 * * *"
		baseUrl="{cms-cm-url}"
		sitemapDirectory="~/sitemaps"
		workingDirectory="~/sitemaps-temp"
		xdt:Transform="SetAttributes(jobCronSchedule, baseUrl, sitemapDirectory, workingDirectory)"
	    xdt:Locator="XPath(/settings/sitemap)" />
	
	<SetAttributeValueTransform
		numberOfEntriesPerFile="4500"
		xdt:Transform="SetAttributes(numberOfEntriesPerFile)"
	    xdt:Locator="XPath(/settings/sitemap/productSitemap)" />		
</transforms>